{
  "name": "bot-tele-api",
  "version": "0.2.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "./node_modules/.bin/sequelize-cli migration:generate",
    "migrate": "./node_modules/.bin/sequelize-cli db:migrate",
    "migrate:undo": "./node_modules/.bin/sequelize-cli db:migrate:undo",
    "migrate:undo:all": "./node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "migrate:refresh": "npm run migrate:undo:all && npm run migrate",
    "seed:generate": "./node_modules/.bin/sequelize-cli seed:generate",
    "seed:run": "./node_modules/.bin/sequelize-cli db:seed:all",
    "seed:undo": "./node_modules/.bin/sequelize-cli db:seed:undo",
    "seed:undo:all": "./node_modules/.bin/sequelize-cli db:seed:undo:all",
    "migrate:refresh:seed": "npm run migrate:refresh && npm run seed:run"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.9",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.9",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.9",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.9",
    "@nestjs/schedule": "^4.1.2",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@types/lodash": "^4.17.5",
    "@types/multer": "^1.4.11",
    "@types/validator": "^13.12.0",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.9.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.6.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.7",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "fastest-levenshtein": "^1.0.16",
    "file-type": "^16.5.3",
    "jimp": "^0.16.1",
    "nanoid": "^3.3.6",
    "nats": "^2.29.1",
    "nats.ws": "^1.30.1",
    "nestjs-cls": "^4.3.0",
    "node-telegram-bot-api": "^0.66.0",
    "otplib": "^12.0.1",
    "p-queue": "6.6.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "password-validator": "^5.3.0",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "qrcode": "^1.5.4",
    "qrcode-reader": "^1.0.4",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.7",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "stripe": "^17.5.0",
    "uniqid": "^5.4.0",
    "websocket": "^1.0.35"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.9",
    "@types/cache-manager": "^4.0.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.5",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "testTimeout": 5000
  }
}
